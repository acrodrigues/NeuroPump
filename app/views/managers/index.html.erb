<h1>Listing managers</h1>

<table>
  <tr>
    <th>Telephone</th>
    <th>Mobilephone</th>
    <th>Gender ID</th>
    <th>Clinic ID</th>
    <th>Login ID</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @managers.each do |manager| %>
  <tr>
    <td><%= manager.telephone %></td>
    <td><%= manager.mobilephone %></td>
    <td><%= manager.gender_id %></td>
    <td><%= manager.clinic_id %></td>
    <td><%= manager.login_id %></td>
    <td><%= link_to 'Show', manager %></td>
    <td><%= link_to 'Edit', edit_manager_path(manager) %></td>
    <td><%= link_to 'Destroy', manager, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Manager', new_manager_path %>
