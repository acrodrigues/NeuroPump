<% if login_signed_in? %>
	<% if Login.find(current_login.id).has_role? :manager %> 
  		<%= render :file => "/clinics/showManager.html.erb" %>
    <% else %>
    	<% if Login.find(current_login.id).has_role? :administrator %> 
   			<%= render :file => "/clinics/showAdmin.html.erb" %>
   		<%end %>	
   <%end %>
<%end %>