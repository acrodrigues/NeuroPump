<h1>Listing civil_statuses</h1>

<table>
  <tr>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @civil_statuses.each do |civil_status| %>
  <tr>
    <td><%= civil_status.description %></td>
    <td><%= link_to 'Show', civil_status %></td>
    <td><%= link_to 'Edit', edit_civil_status_path(civil_status) %></td>
    <td><%= link_to 'Destroy', civil_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

